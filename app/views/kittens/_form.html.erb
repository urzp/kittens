<%= form_for @kitten , url: kitten_path, html: { multipart: true }  do |f| %>

  <p><%= f.file_field :image %></p>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br><br>

  <%= f.label :age %><br>
  <%= f.text_field :age %><br><br>


  <%= f.submit @label, class: "btn btn-primary" %>
<% end %>
